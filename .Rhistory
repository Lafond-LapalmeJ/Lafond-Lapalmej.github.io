# Initialiser le data.frame
data_workshop = data.frame(name = character(),
gender = factor(levels = c("Male","Female")),
writing_hand = factor(levels = c("Right", "Left")),
nb_car = numeric(),
nb_pet = numeric(),
pets_by_car = numeric())
# Pour chaque fichier csv ajouter au data.frame
for(file in csv_path){
print(file)
temp_df <- read.csv(file,row.names = 1)
names(temp_df) = names(data_workshop)
data_workshop = rbind(data_workshop,temp_df)
}
# Sommaire du data.frame
summary(data_workshop)
# Ecrire le data.frame dans un fichier
write.csv(x = data_workshop, file = "/media/common/workshop_r/datasets/data_workshop.csv", row.names = FALSE)
